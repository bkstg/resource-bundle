    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^2.8||^4.0",
        "bkstg/core-bundle": "2.0.x-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Luke Bainbridge",
            "email": "luke.bainbridge@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Bkstg\\ResourceBundle\\": "" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
